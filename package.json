{
  "name": "my-scratch3-offline",
  "version": "0.1.3",
  "description": "OSEP_Code Editor 離線編輯器",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "clean": "rimraf ./app && rimraf ./osep_scratch3-linux-x64 && rimraf ./osep_scratch3-win32-x64",
    "copy": "rimraf ./app && cp -Rf ../osep_scratch-gui/build ./app",
    "package": "npm run clean && electron-packager . OSEP_scratch3 --asar --platform=linux,win32",
    "package:linux": "rimraf ./osep_scratch3-linux-x64 && electron-packager . osep_scratch3 --asar=true --platform=linux",
    "package:win32": "rimraf ./osep_scratch3-win32-x64 && electron-packager . osep_scratch3 --asar=true --platform=win32",
    "package:mac": "rimraf ./osep_scratch3-mac && electron-packager . osep_scratch3 --asar=true --platform=darwin --arch=x64",
    "compress": "npm run compress:linux && npm run compress:win32 && npm run compress:mac",
    "compress:win32": "rimraf osep_scratch3-win32-x64-*.7z && 7z a osep_scratch3-win32-x64-`date '+%Y%m%d%H%M%S'`.7z osep_scratch3-win32-x64",
    "compress:linux": "rimraf osep_scratch3-linux-x64-*.7z && 7z a osep_scratch3-linux-x64-`date '+%Y%m%d%H%M%S'`.7z osep_scratch3-linux-x64",
    "compress:mac": "rimraf osep_scratch3-darwin-x64-*.7z && 7z a osep_scratch3-darwin-x64-`date '+%Y%m%d%H%M%S'`.7z osep_scratch3-darwin-x64"
  },
  "keywords": [
    "scratch3",
    "osep_scratch3",
    "offline"
  ],
  "author": "t301000 <t301000@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "electron": "^2.0.16",
    "electron-packager": "^13.0.1",
    "rimraf": "^2.6.2"
  }
}
